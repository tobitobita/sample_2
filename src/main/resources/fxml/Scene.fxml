<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.web.WebView?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.65" fx:controller="xyz.sakihokore.samplelauncher.FXMLController">
    <children>
      <FlowPane orientation="VERTICAL" prefHeight="200.0" prefWidth="120.0" style="-fx-background-color: #ffcccc;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
              <Button id="excel" onAction="#handleButtonAction" text="エクセル" />
            <Button id="word" mnemonicParsing="false" onAction="#handleButtonAction" text="ワード" />
         </children>
      </FlowPane>
      <WebView fx:id="webView" layoutX="120.0" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="120.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
    </children>
</AnchorPane>
